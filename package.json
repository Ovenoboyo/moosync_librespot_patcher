  },
  "author": "Sahil Gupte",
  "devDependencies": {
    "@moosync/moosync-types": "https://github.com/Moosync/extension-api-types",
    "@moosync/packer": "^0.1.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/tar-stream": "^2.2.2",
    "adm-zip": "^0.5.9",
    "axios": "^1.2.1",
    "gunzip-maybe": "^1.4.2",
    "patch-package": "^6.5.0",
    "sudo-prompt": "^9.2.1",
    "tar-stream": "^2.2.0"
  },
  "exclude": [
    "dist/*"
  ],
  "include": [
    "./dist/index.js",
    "./dist/index.js.map"
